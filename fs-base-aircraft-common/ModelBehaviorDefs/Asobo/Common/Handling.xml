	<Template Name="ASOBO_HANDLING_Switch_Spoiler_Template">
		<Parameters Type="Default">
			<ANIM_NAME>HANDLING_Switch_Spoiler</ANIM_NAME>
			<NODE_ID>HANDLING_Switch_Spoiler</NODE_ID>
			<PART_ID>HANDLING_Switch_Spoiler</PART_ID>
			<TIME_TO_FULL_RANGE_SEC>2</TIME_TO_FULL_RANGE_SEC>
			<REPEAT_FREQUENCY>10</REPEAT_FREQUENCY>
		</Parameters>
		<Parameters Type="Override">
			<INCREMENT Process="Int">16384 #TIME_TO_FULL_RANGE_SEC# / #REPEAT_FREQUENCY# /</INCREMENT>
		</Parameters>
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Switch_3States">
				<CODE_REPEAT_0>
					(A:SPOILERS HANDLE POSITION, Position 16k) #INCREMENT# + 16384 min (&gt;K:SPOILERS_SET)
				</CODE_REPEAT_0>
				<CODE_REPEAT_2>
					(A:SPOILERS HANDLE POSITION, Position 16k) #INCREMENT# - 0 max (&gt;K:SPOILERS_SET)
				</CODE_REPEAT_2>
				<MOMENTARY_SWITCH/>
				<STATE0_TIMER>0.1</STATE0_TIMER>
				<STATE_MAX_TIMER>0</STATE_MAX_TIMER>
				<SWITCH_DIRECTION>Horizontal</SWITCH_DIRECTION>
			</UseTemplate>
		</Component>
	</Template>
