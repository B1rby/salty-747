			<!-- <DIFFERENTIATE_BRAKE_ANIM_L_R>True</DIFFERENTIATE_BRAKE_ANIM_L_R> -->
		</Parameters>
		<UseTemplate Name="ASOBO_HANDLING_RudderPedals_SubTemplate">
