<Template Name="ASOBO_FA18_Digital_Display_Indicator_SubTemplate">
	<DefaultTemplateParameters>
		<PREFIX>#PLANE_PREFIX#_DDI</PREFIX>
		<NODE_ID_GAIN>DDI_Switch_Gain#SUFFIX_ID#</NODE_ID_GAIN>
		<ANIM_NAME_GAIN>DDI_Switch_Gain#SUFFIX_ID#</ANIM_NAME_GAIN>
		<NODE_ID_CONT>DDI_Switch_Cont#SUFFIX_ID#</NODE_ID_CONT>
		<ANIM_NAME_CONT>DDI_Switch_Cont#SUFFIX_ID#</ANIM_NAME_CONT>
		<NODE_ID_SCREEN>DDI_Screen#SUFFIX_ID#</NODE_ID_SCREEN>
	</DefaultTemplateParameters>
	
	<Component ID="#PREFIX##SUFFIX_ID#">
		<UseTemplate Name="ASOBO_GT_Helper_Increasing_ID">
			<EXIT_TEMPLATE>ASOBO_FA18_DDI_Push_Template</EXIT_TEMPLATE>
			<PARAM>BTN_ID</PARAM>
			<MAX_ID>20</MAX_ID>
		</UseTemplate>
		
		<Component ID="#NODE_ID_GAIN#" Node="#NODE_ID_GAIN#">
			<UseTemplate Name="ASOBO_GT_Switch_3States">
				<ANIM_NAME>#ANIM_NAME_GAIN#</ANIM_NAME>
				<CODE_POS_0>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0.03 + 1 min (&gt;I:XMLVAR_#PREFIX#_#ID#_Brightness)
				</CODE_POS_0>
				<CODE_POS_2>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0.03 - 0.05 max (&gt;I:XMLVAR_#PREFIX#_#ID#_Brightness)
				</CODE_POS_2>
				<MOMENTARY_REPEAT_FREQUENCY>5</MOMENTARY_REPEAT_FREQUENCY>
				<MOMENTARY_SWITCH/>
				<STATE_MAX_TIMER>0.01</STATE_MAX_TIMER>
				<STATE0_TIMER>0.01</STATE0_TIMER>
			</UseTemplate>
		</Component>
		
		<UseTemplate Name="ASOBO_GT_Interaction_Tooltip">
			<NODE_ID>#NODE_ID_CONT#</NODE_ID>
			<ANIM_NAME>#ANIM_NAME_CONT#</ANIM_NAME>
			<ANIM_POS>50</ANIM_POS>
		</UseTemplate>
		
		<UseTemplate Name="ASOBO_FA18_Screen_Template">
		</UseTemplate>
	</Component>
</Template>

