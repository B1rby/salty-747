</Template>

<Template Name="ASOBO_FA18_Multipurpose_Color_Display_SubTemplate">
	<DefaultTemplateParameters>
		<PREFIX>#PLANE_PREFIX#_DDI</PREFIX>
		<NODE_ID_SCREEN>MPCD_Screen_#ID#</NODE_ID_SCREEN>
		<DEFAULT_SCREEN_BRIGHTNESS>0.5</DEFAULT_SCREEN_BRIGHTNESS>
	</DefaultTemplateParameters>
	
	<Component ID="#PREFIX##SUFFIX_ID#">
		<UseTemplate Name="ASOBO_GT_Helper_Increasing_ID">
			<EXIT_TEMPLATE>ASOBO_FA18_DDI_Push_Template</EXIT_TEMPLATE>
			<PARAM>BTN_ID</PARAM>
			<MAX_ID>20</MAX_ID>
		</UseTemplate>
		
		<Component ID="#NODE_ID_GAIN#" Node="#NODE_ID_GAIN#">
			<UseTemplate Name="ASOBO_GT_Knob_Finite_Code">
				<ANIM_NAME>#ANIM_NAME_GAIN#</ANIM_NAME>
				<ANIM_CODE>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 100 *
				</ANIM_CODE>
				<CLOCKWISE_CODE>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0.05 + 1 min (&gt;I:XMLVAR_#PREFIX#_#ID#_Brightness)
				</CLOCKWISE_CODE>
				<ANTICLOCKWISE_CODE>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0.05 - 0.05 max (&gt;I:XMLVAR_#PREFIX#_#ID#_Brightness)
				</ANTICLOCKWISE_CODE>
			</UseTemplate>
		</Component>
		
		<UseTemplate Name="ASOBO_GT_Interaction_Tooltip">
			<NODE_ID>#NODE_ID_CONT#</NODE_ID>
			<ANIM_NAME>#ANIM_NAME_CONT#</ANIM_NAME>
			<ANIM_POS>50</ANIM_POS>
		</UseTemplate>
		
		<UseTemplate Name="ASOBO_FA18_Screen_Template">
		</UseTemplate>
	</Component>
</Template>

<Template Name="ASOBO_FA18_Screen_Template">
	<DefaultTemplateParameters>
		<DEFAULT_SCREEN_BRIGHTNESS>0.5</DEFAULT_SCREEN_BRIGHTNESS>
		<BRIGHTNESS_VARIABLE_TYPE>I</BRIGHTNESS_VARIABLE_TYPE>
		<BRIGHTNESS_VARIABLE_NAME>XMLVAR_#PREFIX#_#ID#_Brightness</BRIGHTNESS_VARIABLE_NAME>
	</DefaultTemplateParameters>
	
	<UseTemplate Name="ASOBO_GT_Update">
		<UPDATE_ONCE>True</UPDATE_ONCE>
		<UPDATE_CODE>
			#DEFAULT_SCREEN_BRIGHTNESS# (&gt;#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#)
		</UPDATE_CODE>
	</UseTemplate>
	<UseTemplate Name="ASOBO_GT_Component_Emissive_Gauge">
		<NODE_ID>#NODE_ID_SCREEN#</NODE_ID>
		<EMISSIVE_CODE>(#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#)</EMISSIVE_CODE>
	</UseTemplate>
</Template>

