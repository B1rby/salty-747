<Template Name="ASOBO_FA18_Knob_Brightness_Template">
	<DefaultTemplateParameters>
		<BRIGHTNESS_VARIABLE_TYPE>I</BRIGHTNESS_VARIABLE_TYPE>
		<BRIGHTNESS_VARIABLE_NAME>XMLVAR_#PREFIX#_#ID#_Brightness</BRIGHTNESS_VARIABLE_NAME>
	</DefaultTemplateParameters>
	<OverrideTemplateParameters>
		<Condition Valid="CIRCUIT">
			<True>
				<CIRCUIT_CODE_ON>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0 &gt;
					(A:CIRCUIT SWITCH ON:#CIRCUIT#, Boolean) ! and if{
						#CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_TOGGLE)
					}
				</CIRCUIT_CODE_ON>
				<CIRCUIT_CODE_OFF>
					(I:XMLVAR_#PREFIX#_#ID#_Brightness) 0 ==
					(A:CIRCUIT SWITCH ON:#CIRCUIT#, Boolean) and if{
						#CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_TOGGLE)
					}
				</CIRCUIT_CODE_OFF>
			</True>
			<False>
				<CIRCUIT_CODE_ON></CIRCUIT_CODE_ON>
				<CIRCUIT_CODE_OFF></CIRCUIT_CODE_OFF>
			</False>
		</Condition>
	</OverrideTemplateParameters>

	<Component ID="#NODE_ID#" Node="#NODE_ID#">
		<UseTemplate Name="ASOBO_GT_Knob_Finite_Code">
			<ANIM_CODE>
				(#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#) 100 *
			</ANIM_CODE>
			<CLOCKWISE_CODE>
				(#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#) 0.03 + 1 min (&gt;#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#)
				#CIRCUIT_CODE_ON#
			</CLOCKWISE_CODE>
			<ANTICLOCKWISE_CODE>
				(#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#) 0.03 - 0.0 max (&gt;#BRIGHTNESS_VARIABLE_TYPE#:#BRIGHTNESS_VARIABLE_NAME#)
				#CIRCUIT_CODE_OFF#
			</ANTICLOCKWISE_CODE>
		</UseTemplate>
	</Component>
</Template>

<Template Name="ASOBO_FA18_UFCD_COM_Knobs_Template">
	<DefaultTemplateParameters>
		<COM_ID>1</COM_ID>
	</DefaultTemplateParameters>
	<OverrideTemplateParameters>
		<Condition Valid="COM_CIRCUIT">
			<True>
				<CIRCUIT_CODE_ON>
					(A:COM VOLUME:#COM_ID#, Percent) 0 &gt;
					(A:CIRCUIT SWITCH ON:#COM_CIRCUIT#, Boolean) ! and if{
						#COM_CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_TOGGLE)
					}
				</CIRCUIT_CODE_ON>
				<CIRCUIT_CODE_OFF>
					(A:COM VOLUME:#COM_ID#, Percent) 0 ==
					(A:CIRCUIT SWITCH ON:#COM_CIRCUIT#, Boolean) and if{
						#COM_CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_TOGGLE)
					}
				</CIRCUIT_CODE_OFF>
			</True>
			<False>
				<CIRCUIT_CODE_ON></CIRCUIT_CODE_ON>
				<CIRCUIT_CODE_OFF></CIRCUIT_CODE_OFF>
			</False>
		</Condition>
	</OverrideTemplateParameters>

	<Component ID="#NODE_ID_KNOB_COM_VOLUME#" Node="#NODE_ID_KNOB_COM_VOLUME#">
		<UseTemplate Name="ASOBO_GT_Knob_Finite_Simvar">
			<ANIM_SIMVAR>COM VOLUME:#COM_ID#</ANIM_SIMVAR>
			<ANIM_SIMVAR_UNITS>Percent</ANIM_SIMVAR_UNITS>
			<ANIM_NAME>#ANIM_NAME_KNOB_COM_VOLUME#</ANIM_NAME>
			<CLOCKWISE_CODE>
				(&gt;K:COM#COM_ID#_VOLUME_INC)
				#CIRCUIT_CODE_ON#
			</CLOCKWISE_CODE>
			<ANTICLOCKWISE_CODE>
				(&gt;K:COM#COM_ID#_VOLUME_DEC)
				#CIRCUIT_CODE_OFF#
			</ANTICLOCKWISE_CODE>
		</UseTemplate>
	</Component>
	<Component ID="#NODE_ID_KNOB_COM#" Node="#NODE_ID_KNOB_COM#">
		<UseTemplate Name="ASOBO_GT_Knob_Infinite">
			<ANIM_NAME>#ANIM_NAME_KNOB_COM#</ANIM_NAME>
			<CLOCKWISE_CODE>
				(&gt;H:#PREFIX#_#ID#_KNOB_COM#COM_ID#_INC)
			</CLOCKWISE_CODE>
			<ANTICLOCKWISE_CODE>
				(&gt;H:#PREFIX#_#ID#_KNOB_COM#COM_ID#_DEC)
			</ANTICLOCKWISE_CODE>
		</UseTemplate>
	</Component>
</Template>

<Template Name="ASOBO_FA18_Multipurpose_Color_Display_Template">
