<Include Path="Asobo\Generic.xml"/>

<Template Name="ASOBO_FA18_Lights_Ident_Switch_Template">
	<DefaultTemplateParameters>
		<NODE_ID>FA18_Lights_Ident_Switch</NODE_ID>
		<ANIM_NAME>FA18_Lights_Ident_Switch</ANIM_NAME>
		<SWITCH_DIRECTION>Horizontal</SWITCH_DIRECTION>
		<ARROW_TYPE>Curved</ARROW_TYPE>
		<TOTAL_DURATION_LIST/>
		<1_FLASH_COUNT_LIST/>
		<1_WAIT_DURATION_LIST/>
		<1_FLASH_DURATION_LIST/>
		<2_FLASH_COUNT_LIST/>
		<2_WAIT_DURATION_LIST/>
		<2_FLASH_DURATION_LIST/>
		<INPUT_POTENTIOMETER>17</INPUT_POTENTIOMETER>
		<OUTPUT_POTENTIOMETER>18</OUTPUT_POTENTIOMETER>
	</DefaultTemplateParameters>
	
	<UseTemplate Name="ASOBO_FA18_Lights_Ident_Switch_Param_Processing_Template">
		<NEXT_ID>1</NEXT_ID>
	</UseTemplate>
</Template>

<Template Name="ASOBO_FA18_Lights_Ident_Switch_Param_Processing_Template">
	<OverrideTemplateParameters>
		<CURRENT_1_WAIT_DURATION Process="Param">#NEXT_ID#_1_WAIT_DURATION</CURRENT_1_WAIT_DURATION>
		<CURRENT_2_WAIT_DURATION Process="Param">#NEXT_ID#_2_WAIT_DURATION</CURRENT_2_WAIT_DURATION>
		<CURRENT_1_FLASH_COUNT Process="Param">#NEXT_ID#_1_FLASH_COUNT</CURRENT_1_FLASH_COUNT>
		<CURRENT_2_FLASH_COUNT Process="Param">#NEXT_ID#_2_FLASH_COUNT</CURRENT_2_FLASH_COUNT>
	</OverrideTemplateParameters>
	
	<Condition Check="#NEXT_ID#_1_WAIT_DURATION">
		<True>
			<UseTemplate Name="ASOBO_FA18_Lights_Ident_Switch_Param_Processing_2_Template">
				<CURRENT_TOTAL_DURATION Process="Int">#CURRENT_1_WAIT_DURATION# #CURRENT_2_WAIT_DURATION# + #FLASH_DURATION# #CURRENT_1_FLASH_COUNT# 2 * 1 - 0 max * + #FLASH_DURATION# #CURRENT_2_FLASH_COUNT# 2 * 1 - 0 max * +</CURRENT_TOTAL_DURATION>
				<CURRENT_1_FLASH_DURATION Process="Int">#FLASH_DURATION# #CURRENT_1_FLASH_COUNT# 2 * 1 - 0 max *</CURRENT_1_FLASH_DURATION>
				<CURRENT_2_FLASH_DURATION Process="Int">#FLASH_DURATION# #CURRENT_2_FLASH_COUNT# 2 * 1 - 0 max *</CURRENT_2_FLASH_DURATION>
			</UseTemplate>
		</True>
		<False>
			<UseTemplate Name="ASOBO_FA18_Lights_Ident_Switch_SubTemplate">
			</UseTemplate>
		</False>
	</Condition>
</Template>

<Template Name="ASOBO_FA18_Lights_Ident_Switch_Param_Processing_2_Template">
	<UseTemplate Name="ASOBO_FA18_Lights_Ident_Switch_Param_Processing_Template">
		<NEXT_ID Process="Int">#NEXT_ID# 1 +</NEXT_ID>
		<TOTAL_DURATION_LIST>#CURRENT_TOTAL_DURATION# #TOTAL_DURATION_LIST#</TOTAL_DURATION_LIST>
		<1_FLASH_COUNT_LIST>#CURRENT_1_FLASH_COUNT# #1_FLASH_COUNT_LIST#</1_FLASH_COUNT_LIST>
		<1_WAIT_DURATION_LIST>#CURRENT_1_WAIT_DURATION# #1_WAIT_DURATION_LIST#</1_WAIT_DURATION_LIST>
		<1_FLASH_DURATION_LIST>#CURRENT_1_FLASH_DURATION# #1_FLASH_DURATION_LIST#</1_FLASH_DURATION_LIST>
		<2_FLASH_COUNT_LIST>#CURRENT_2_FLASH_COUNT# #2_FLASH_COUNT_LIST#</2_FLASH_COUNT_LIST>
		<2_WAIT_DURATION_LIST>#CURRENT_2_WAIT_DURATION# #2_WAIT_DURATION_LIST#</2_WAIT_DURATION_LIST>
		<2_FLASH_DURATION_LIST>#CURRENT_2_FLASH_DURATION# #2_FLASH_DURATION_LIST#</2_FLASH_DURATION_LIST>
	</UseTemplate>
</Template>

<Template Name="ASOBO_FA18_Lights_Ident_Switch_SubTemplate">
	<DefaultTemplateParameters>
		<EXTRA_UPDATE_CODE></EXTRA_UPDATE_CODE>
	</DefaultTemplateParameters>
	<OverrideTemplateParameters>
		<MAX_ID Process="Int">#NEXT_ID# 1 -</MAX_ID>
	</OverrideTemplateParameters>
	<Component ID="#NODE_ID#" Node="#NODE_ID#">
		<UseTemplate Name="ASOBO_GT_Update">
			<FREQUENCY>30</FREQUENCY>
			<UPDATE_CODE>
				(E:SIMULATION TIME, seconds)  100 * flr sp0
				#TOTAL_DURATION_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp2
				#1_FLASH_COUNT_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp3
				#1_WAIT_DURATION_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp4
				#1_FLASH_DURATION_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp5
				#2_FLASH_COUNT_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp6
				#2_WAIT_DURATION_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp7
				#2_FLASH_DURATION_LIST# #MAX_ID# (L:XMLVAR_EXT_LTS_IDENT) case sp8
				
				l0 l2 % sp1

				0 sp9
				l1 l5 &lt; if{
					l1 #FLASH_DURATION# / flr 2 % 0 == sp9
				}
				els{
					l1 l4 l5 + - d 0 &gt; r l8 &lt; and if{
						l1 #FLASH_DURATION# / flr 2 % 0 == sp9
					}
				}
				
				(A:LIGHT POTENTIOMETER:#INPUT_POTENTIOMETER#, Percent over 100) l9 * #OUTPUT_POTENTIOMETER# (&gt;K:2:LIGHT_POTENTIOMETER_SET)
				#EXTRA_UPDATE_CODE#
			</UPDATE_CODE>
		</UseTemplate>
		<UseTemplate Name="ASOBO_GT_Switch_XStates">
			<NUM_STATES>7</NUM_STATES>
			<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
			<SWITCH_POSITION_VAR>XMLVAR_EXT_LTS_IDENT</SWITCH_POSITION_VAR>
		</UseTemplate>
	</Component>
</Template>

