<Template Name="ASOBO_FA18_HUD_Control_Panel_SubTemplate">
	<DefaultTemplateParameters>
		<PREFIX>#PLANE_PREFIX#_HUD</PREFIX>
		<NODE_ID_CLUTTER>HUD_Switch_Clutter#SUFFIX_ID#</NODE_ID_CLUTTER>
		<ANIM_NAME_CLUTTER>HUD_Switch_Clutter#SUFFIX_ID#</ANIM_NAME_CLUTTER>
		<NODE_ID_BRT>HUD_Knob_Brightness#SUFFIX_ID#</NODE_ID_BRT>
		<ANIM_NAME_BRT>HUD_Knob_Brightness#SUFFIX_ID#</ANIM_NAME_BRT>
		<NODE_ID_DAY_NIGHT>HUD_Switch_DayNight#SUFFIX_ID#</NODE_ID_DAY_NIGHT>
		<ANIM_NAME_DAY_NIGHT>HUD_Switch_DayNight#SUFFIX_ID#</ANIM_NAME_DAY_NIGHT>
		<NODE_ID_BLK_LVL>HUD_Knob_Black_Level#SUFFIX_ID#</NODE_ID_BLK_LVL>
		<ANIM_NAME_BLK_LVL>HUD_Knob_Black_Level#SUFFIX_ID#</ANIM_NAME_BLK_LVL>
		<NODE_ID_NVG_PWR>HUD_Knob_NVG_Pwr#SUFFIX_ID#</NODE_ID_NVG_PWR>
		<ANIM_NAME_NVG_PWR>HUD_Knob_NVG_Pwr#SUFFIX_ID#</ANIM_NAME_NVG_PWR>
		<NODE_ID_BAT>HUD_Knob_BAT#SUFFIX_ID#</NODE_ID_BAT>
		<ANIM_NAME_BAT>HUD_Knob_BAT#SUFFIX_ID#</ANIM_NAME_BAT>
		<NODE_ID_AOA>HUD_Knob_AOA#SUFFIX_ID#</NODE_ID_AOA>
		<ANIM_NAME_AOA>HUD_Knob_AOA#SUFFIX_ID#</ANIM_NAME_AOA>
		<NODE_ID_ALT>HUD_Knob_ALT#SUFFIX_ID#</NODE_ID_ALT>
		<ANIM_NAME_ALT>HUD_Knob_ALT#SUFFIX_ID#</ANIM_NAME_ALT>
		<NODE_ID_ATT>HUD_Knob_ATT#SUFFIX_ID#</NODE_ID_ATT>
		<ANIM_NAME_ATT>HUD_Knob_ATT#SUFFIX_ID#</ANIM_NAME_ATT>
		<NODE_ID_SCREEN>HUD_Screen</NODE_ID_SCREEN>
	</DefaultTemplateParameters>
	
	<Component ID="#PREFIX##SUFFIX_ID#">
		<Component ID="#NODE_ID_CLUTTER#" Node="#NODE_ID_CLUTTER#">
			<UseTemplate Name="ASOBO_GT_Switch_3States">
				<ANIM_NAME>#ANIM_NAME_CLUTTER#</ANIM_NAME>
				<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
				<SWITCH_POSITION_VAR>XMLVAR_#PREFIX#_#ID#_Clutter</SWITCH_POSITION_VAR>
			</UseTemplate>
		</Component>
		
		<UseTemplate Name="ASOBO_FA18_Knob_Brightness_Template">
			<CIRCUIT>#BRT_CIRCUIT#</CIRCUIT>
			<NODE_ID>#NODE_ID_BRT#</NODE_ID>
			<ANIM_NAME>#ANIM_NAME_BRT#</ANIM_NAME>
		</UseTemplate>
		
		<Component ID="#NODE_ID_DAY_NIGHT#" Node="#NODE_ID_DAY_NIGHT#">
			<UseTemplate Name="ASOBO_GT_Switch_Code">
				<ANIM_NAME>#ANIM_NAME_DAY_NIGHT#</ANIM_NAME>
				<ANIM_CODE>(L:XMLVAR_#PREFIX#_#ID#_NightMode) 100 *</ANIM_CODE>
				<LEFT_SINGLE_CODE>(L:XMLVAR_#PREFIX#_#ID#_NightMode) ! (&gt;L:XMLVAR_#PREFIX#_#ID#_NightMode)</LEFT_SINGLE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_BLK_LVL#" Node="#NODE_ID_BLK_LVL#">
			<UseTemplate Name="ASOBO_GT_Knob_Infinite">
				<ANIM_NAME>#ANIM_NAME_BLK_LVL#</ANIM_NAME>
				<CLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_Black_Level) 0.03 + 1 min (&gt;L:XMLVAR_#PREFIX#_#ID#_Black_Level)
				</CLOCKWISE_CODE>
				<ANTICLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_Black_Level) 0.03 - 0.0 max (&gt;L:XMLVAR_#PREFIX#_#ID#_Black_Level)
				</ANTICLOCKWISE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_NVG_PWR#" Node="#NODE_ID_NVG_PWR#">
			<UseTemplate Name="ASOBO_GT_Switch_3States">
				<ANIM_NAME>#ANIM_NAME_NVG_PWR#</ANIM_NAME>
				<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
				<SWITCH_POSITION_VAR>XMLVAR_#PREFIX#_#ID#_NVG_PWR</SWITCH_POSITION_VAR>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_BAT#" Node="#NODE_ID_BAT#">
			<UseTemplate Name="ASOBO_GT_Knob_Finite_Code">
				<ANIM_NAME>#ANIM_NAME_BAT#</ANIM_NAME>
				<ANIM_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_Brightness_Stroke_To_Raster) 100 *
				</ANIM_CODE>
				<CLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_Brightness_Stroke_To_Raster) 0.03 + 1 min (&gt;L:XMLVAR_#PREFIX#_#ID#_Brightness_Stroke_To_Raster)
				</CLOCKWISE_CODE>
				<ANTICLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_Brightness_Stroke_To_Raster) 0.03 - 0.0 max (&gt;L:XMLVAR_#PREFIX#_#ID#_Brightness_Stroke_To_Raster)
				</ANTICLOCKWISE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_AOA#" Node="#NODE_ID_AOA#">
			<UseTemplate Name="ASOBO_GT_Knob_Finite_Code">
				<ANIM_NAME>#ANIM_NAME_AOA#</ANIM_NAME>
				<ANIM_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_AOA) 100 *
				</ANIM_CODE>
				<CLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_AOA) 0.03 + 1 min (&gt;L:XMLVAR_#PREFIX#_#ID#_AOA)
				</CLOCKWISE_CODE>
				<ANTICLOCKWISE_CODE>
					(L:XMLVAR_#PREFIX#_#ID#_AOA) 0.03 - 0.0 max (&gt;L:XMLVAR_#PREFIX#_#ID#_AOA)
				</ANTICLOCKWISE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_ALT#" Node="#NODE_ID_ALT#">
			<UseTemplate Name="ASOBO_GT_Switch_Code">
				<ANIM_NAME>#ANIM_NAME_ALT#</ANIM_NAME>
				<ANIM_CODE>(L:XMLVAR_#PREFIX#_#ID#_ALT) 100 *</ANIM_CODE>
				<LEFT_SINGLE_CODE>(L:XMLVAR_#PREFIX#_#ID#_ALT) ! (&gt;L:XMLVAR_#PREFIX#_#ID#_ALT)</LEFT_SINGLE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#NODE_ID_ATT#" Node="#NODE_ID_ATT#">
			<UseTemplate Name="ASOBO_GT_Switch_3States">
				<ANIM_NAME>#ANIM_NAME_ATT#</ANIM_NAME>
				<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
				<SWITCH_POSITION_VAR>XMLVAR_#PREFIX#_#ID#_ATT</SWITCH_POSITION_VAR>
			</UseTemplate>
		</Component>
		
		<UseTemplate Name="ASOBO_FA18_Screen_Template">
		</UseTemplate>
	</Component>
</Template>

